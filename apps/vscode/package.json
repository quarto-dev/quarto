        "category": "Quarto"
      },
      {
        "command": "quarto.runCellsBelow",
        "title": "Run Cells Below",
        "category": "Quarto"
      },
      {
        "command": "quarto.runAllCells",
        "title": "Run All Cells",
        "category": "Quarto"
      },
