      },
      {
        "command": "quarto.runPreviousCell",
        "title": "Run Previous Cell",
        "category": "Quarto"
      },
      {
        "command": "quarto.runNextCell",
        "title": "Run Next Cell",
        "category": "Quarto"
      },
      {
        "command": "quarto.runCellsAbove",
